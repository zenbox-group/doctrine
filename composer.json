{
    "name": "zenbox/doctrine",
    "description": "Doctrine extensions",
    "keywords": ["doctrine", "extensions"],
    "homepage": "https://github.com/zenbox-group/doctrine",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "ZenBox\\Doctrine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZenBox\\Doctrine\\Test\\": "test/"
        }
    },
    "require": {
        "php": "^7.1",
        "psr/container": "^1.0",
        "doctrine/data-fixtures": "^1.3",
        "doctrine/migrations": "^1.8",
        "doctrine/orm": "^2.6",
        "symfony/console": "^4.1 || ^5.1",
        "symfony/yaml": "^4.1 || ^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0.1",
        "symfony/var-dumper": "^4.2 || ^5.1"
    }
}
